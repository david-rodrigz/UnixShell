#!/bin/bash

# set the program name
prg=./try

for t in Test/Test_* ; do
    echo ${t##*/}
    # $prg <$t/inp 2>&1 >$t/out
    source $t/test.sh
    run_${t##*/} $t
    # diff -q -w $t/exp $t/out 2>&1 >/dev/null || echo FAILED >&2
    if diff -q -w $t/exp $t/out 2>&1 >/dev/null; then
        echo "PASSED"
    else
        echo "FAILED" >&2
    fi
    echo ""
done
